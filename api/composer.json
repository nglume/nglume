{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/spira/Elasticquent"
        },
        {
            "type": "vcs",
            "url": "https://github.com/spira/revisionable"
        }
    ],
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "bosnadev/database": "^0.11.0",
        "illuminate/mail": "^5.0",
        "jeremeamia/superclosure": "^2.1",
        "pda/pheanstalk": "^3.0",
        "predis/predis": "^1.0",
        "league/fractal": "0.12.*",
        "illuminate/redis": "5.1.*",
        "guzzlehttp/guzzle": "5.3.*",
        "league/flysystem": "^1.0",
        "tymon/jwt-auth": "0.5.*",
        "laravel/socialite": "^2.0",
        "cloudinary/cloudinary_php": "^1.1",
        "elasticquent/elasticquent": "dev-master",
        "venturecraft/revisionable": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "^0.9.4",
        "fzaninotto/faker": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Spira\\": "src/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "post-install-cmd": [
            "npm install",
            "php artisan auth:generate-keys"
        ]
    }
}
