{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/spira/Elasticquent"
        },
        {
            "type": "vcs",
            "url": "https://github.com/spira/api-core"
        },
        {
            "type": "vcs",
            "url": "https://github.com/spira/revisionable"
        },
        {
            "type": "vcs",
            "url": "http://github.com/spira/database"
        }
    ],
    "require": {
        "jeremeamia/superclosure": "^2.1",
        "pda/pheanstalk": "^3.0",
        "predis/predis": "^1.0",
        "illuminate/redis": "5.2.*",
        "league/flysystem": "^1.0",
        "laravel/socialite": "^2.0",
        "cloudinary/cloudinary_php": "^1.1",
        "venturecraft/revisionable": "^2.0",
        "spira/core": "dev-feature/lumen-5.2-upgrade",
        "namshi/jose": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.1",
        "mockery/mockery": "^0.9.4",
        "fzaninotto/faker": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Spira\\": "src/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "discard-changes": true
    },
    "scripts": {
        "post-install-cmd": [
            "npm install",
            "php artisan auth:generate-keys"
        ]
    }
}
